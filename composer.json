{
    "name": "daalder/feeds",
    "description": "Daalder package for generating Product feeds (Google, Netrivals, and more).",
    "type": "library",
    "license": "closed source",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/pionect/"
        },
        {
            "packagist.org": false
        }
    ],
    "require": {
        "ext-zip": "*",
        "php": "^7.4|^8.0",
        "pionect/daalder": "^15|^16|^17|dev-acceptance|dev-master|dev-develop"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "orchestra/database": "6.x-dev",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Daalder\\Feeds\\": "src/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daalder\\Feeds\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Daalder\\Feeds\\FeedsServiceProvider"
            ]
        }
    }
}
